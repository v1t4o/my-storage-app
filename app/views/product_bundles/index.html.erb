<h2>Kits de produtos cadastrados</h2>
<div class="row">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Nome</th>
      </tr>
    </thead>
    <tbody>
      <% @product_bundles.each do |product_bundle| %>
        <tr>
          <td><%= link_to product_bundle.name, product_bundle %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>